{
    "name": "herrera-io/file-locator",
    "description": "A simple file locator library.",
    "keywords": ["file", "locator"],
    "homepage": "https://herrera-io.github.com/php-file-locator",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Herrera",
            "email": "kevin@herrera.io",
            "homepage": "http://kevin.herrera.io"
        }
    ],
    "support": {
        "issues": "https://github.com/herrera-io/php-file-locator/issues"
    },
    "require": {
        "php": ">=5.3.3"
    },
    "autoload": {
        "psr-0": {
            "Herrera\\FileLocator": "src/lib"
        }
    },
    "config": {
        "vendor-dir": "src/vendors"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}